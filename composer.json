{
    "name": "smart-php/laravel-cred-api-builder",
    "description": "A package for Laravel to build CRUD REST APIs.",
    "type": "library",
    "license": "MIT",
    "version":"0.0.1",
    "autoload": {
        "psr-4": {
            "Laravel\\CredApiBuilder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\CredApiBuilder\\CredApiBuilderServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "laravel/framework": "^11.31"
    }
}
